<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tool="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:orientation="horizontal"
    tool:context="com.kiki.android.com.kiki.View.Activity.TransModeMainActivity">

    <fragment
        android:id="@+id/transversemode_titles"
        class="com.kiki.android.com.kiki.View.UIFrame.TitleFragement"
        android:layout_width="0px"
        android:layout_height="match_parent"
        android:layout_weight="1">

    </fragment>
    <FrameLayout
        android:id="@+id/transversemode_details"
        android:layout_width="0px"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:background="?android:attr/detailsElementBackground"></FrameLayout>
</LinearLayout>
